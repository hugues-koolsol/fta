                declare(e , 123.456),
                declare(i , 0),
                appelf(n(alert) , p('Fake') , r(b)),
                appelf(n(console.log) , p(b)),
                boucle(
                  initialisation(affecte(i , 0)),
                  condition(inf(i , 5)),
                  increment(affecte(i , i+1)),
                  faire(appelf(n(dansFaireRetourneTrue)) , affecte(e , 456.789))
                ),
                choix(
                  si(
                    condition(
                      (vrai),
                      et(
