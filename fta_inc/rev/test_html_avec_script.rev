                        et(appelf(n(blabla)))
                      )
                    ),
                    alors(appelf(n(alert) , p('OK1')))
                  ),
                  sinonsi(
                    condition(non(egal(true , d))),
                    alors(appelf(n(alert) , p('OK2')))
                  ),
                  sinon(
                    alors(appelf(n(alert) , p('OK3')))
                  )
                )
              )
            )
